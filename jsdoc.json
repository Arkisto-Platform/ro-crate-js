{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": [
      "README.md",
      "lib/types.d.ts",
      "./lib/node.js",
      "./lib/rocrate.js",
      "lib/utils.js"
    ]
  },
  "plugins": ["plugins/markdown"],
  "sourceType": "module",
  "opts": {
    "readme": "./README.md",
    "recurse": true,
    "destination": "./docs",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "light"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
